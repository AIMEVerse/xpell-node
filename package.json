{
  "name": "xpell-node",
  "version": "0.0.12",
  "description": "Xpell Real-Time Interpreter for Node.js",
  "files": [
    "dist",
    "src",
    "types",
    "docs"
  ],
  "type": "module",
  "types": "./dist/xpell.d.ts",
  "main": "./dist/xpell.js",
  "module": "./dist/xpell.js",
  "scripts": {
    "test": "tsc -p . && node ./dist/test.js",
