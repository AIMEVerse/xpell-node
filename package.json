    "type": "git",
    "url": "git+https://github.com/fridman-tamir/Xpell-Core"
  },
  "devDependencies": {
    "@types/core-js": "^2.5.5",
    "@types/node": "^14.18.36",
    "typescript": "^4.9.5"
  }
}
